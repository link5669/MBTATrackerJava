    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <ChoiceBox fx:id="linePicker" prefWidth="150.0" />
   <ChoiceBox fx:id="stopPicker" prefWidth="150.0" />

    <Label fx:id="welcomeText" />
    <Button onAction="#onRefreshButtonClick" text="See Status!" />
</VBox>
